<?php
//passes: ReindentObjOps
$func = function () {
	return method($var1)
		->method($var2)
		->method($var3);
};

$func = function () {
	return method($var1)->method($foo)
		->method($var2)
		->method($var3);
};

return method($var1)
	->method($var2)
	->method($var3);

return method($var1)->method($foo)
	->method($var2)
	->method($var3);

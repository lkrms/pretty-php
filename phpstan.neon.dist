includes:
  - phpstan-baseline.neon

parameters:
  level: 6
  paths:
    - bin
    - bin/pretty-php
    - bootstrap.php
    - src
    - tests
  excludePaths:
    - tests/temp.php
  bootstrapFiles:
    - tests/bootstrap.php
  ignoreErrors:
    - "#^Method Lkrms\\\\Pretty\\\\Tests\\\\.+ (has no return type specified|return type has no value type specified in iterable type array)\\.$#"

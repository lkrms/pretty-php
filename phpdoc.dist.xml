<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:schemaLocation="https://www.phpdoc.org https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd">
    <title>lkrms/pretty-php</title>
    <paths>
        <output>docs/api</output>
        <cache>build/cache/api</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <visibility>public</visibility>
            <visibility>protected</visibility>
            <default-package-name>Lkrms\Pretty</default-package-name>
            <include-source>true</include-source>
            <ignore-tags>
                <ignore-tag>property</ignore-tag>
                <ignore-tag>property-read</ignore-tag>
                <ignore-tag>property-write</ignore-tag>
                <ignore-tag>lkrms-generate-command</ignore-tag>
                <ignore-tag>psalm-param</ignore-tag>
                <ignore-tag>psalm-require-extends</ignore-tag>
                <ignore-tag>psalm-require-implements</ignore-tag>
                <ignore-tag>psalm-return</ignore-tag>
                <ignore-tag>psalm-var</ignore-tag>
            </ignore-tags>
        </api>
    </version>
    <setting
            name="graphs.enabled"
            value="true" />
</phpdocumentor>
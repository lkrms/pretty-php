<?php
// new array syntax
$object
    ->firstMethod()
    ->chainedMethod($var, [
        1,
        2,
        3,
    ]);

$object
    ->firstMethod()
    ->chainedMethod($var, [
        1,
        2,
        3,
    ])
;

// old array syntax
$object
    ->firstMethod()
    ->chainedMethod($var, array(
        1,
        2,
        3,
    ));

$object
    ->firstMethod(
        $obj2
        ->doMagic()
    )
    ->chainedMethod($var, array(
        1,
        2,
        3,
    ))
;



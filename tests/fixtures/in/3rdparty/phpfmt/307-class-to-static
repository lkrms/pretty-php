<?php
//passes:Default,ClassToStatic

class SomeClass {
	const AConstant = SomeClass::constant;
}

class SomeClass2 {
	public $bar = SomeClass2::constant;
}

class SomeClass3 {
	public function a($a = SomeClass3::AnotherConstant) {}
}

class SomeClass4 {
	public function b() {
		SomeClass4::func();
	}
}

class SomeClass5 {
	public function c() {
		SomeClass5::class;
	}
}

class SomeClass6 {
	public function d() {
		$x instanceof SomeClass6;
	}
}

class SomeClass7 {
	public function e() {
		new SomeClass7();
	}
}

interface SomeInterface {
	const AnotherConstant = SomeClass::constant;
	public function f($a = SomeInterface::AnotherConstant);
}

class SomeClass8 {
	const SomeClass8 = 1;
}

class SomeClass9 {
	public function SomeClass9() {}
}

class SomeClass10 {
	public function h() {
		new \NS\SomeClass10();
	}
}

class SomeClass11 {
	public function i() {
		new SomeClass11\SNS();
	}
}

trait SomeTrait {
	public function j() {
		SomeTrait::func();
	}
}
